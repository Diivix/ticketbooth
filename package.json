{
  "name": "ticketbooth",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon -r dotenv/config src/index.ts",
    "test": "jest",
    "start": "node --optimize_for_size --max_old_space_size=256 --gc_interval=100 src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed:all": "npx sequelize-cli db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed:all",
    "createkeys": "rm -rf ./keys && mkdir keys && openssl genrsa -out keys/private.key 2048 && openssl rsa -in keys/private.key -outform PEM -pubout -out keys/public.key",
    "pretest": "npm run migrate:reset",
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "@types/express-validator": "^3.0.0",
    "bcrypt": "^3.0.8",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^5.21.9",
    "sqlite3": "^4.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.9",
    "@types/passport": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-jwt": "^3.0.3",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
