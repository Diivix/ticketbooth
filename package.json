{
  "name": "ticketbooth",
  "version": "0.1.0",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "seed:all": "npx sequelize-cli db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed:all",
    "createkeys": "rm -rf ./keys && mkdir keys && openssl genrsa -out keys/private.key 2048 && openssl rsa -in keys/private.key -outform PEM -pubout -out keys/public.key",
    "pretest": "npm run migrate:reset",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.9",
    "sqlite3": "^4.2.0",
    "typeorm": "^0.2.25",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/concurrently": "^5.2.1",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.6",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.9",
    "@types/passport": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-jwt": "^3.0.3",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
